{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@commands/*": ["./src/commands/*"],
      "@events/*": ["./src/events/*"],
      "@locales/*": ["./src/locales/*"]
    }
  }
}
